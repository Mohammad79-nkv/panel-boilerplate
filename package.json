{
  "name": "sbss-new",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build-langs": "npx babel-node src/utils/compound-intl.ts",
    "watch-langs": "nodemon --watch src/**/en.json --watch src/**/fa.json  --exec npm run build-langs",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>/src$1",
      "^@components(.*)$": "<rootDir>/src/app/components$1",
      "^@app(.*)$": "<rootDir>/src/app$1",
      "^@containers(.*)$": "<rootDir>/src/app/containers$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@enums(.*)$": "<rootDir>/src/enums",
      "^@endpoints(.*)$": "<rootDir>/src/endpoints",
      "^@store(.*)$": "<rootDir>/src/store"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.7",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "antd": "^5.9.3",
    "axios": "^1.4.0",
    "axios-retry": "^3.4.0",
    "babel-plugin-import": "^1.13.8",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "framer-motion": "^10.12.8",
    "glob": "^10.3.7",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "jwt-decode": "^3.1.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.0.1",
    "pkce-challenge": "^3.1.0",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.2",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.2",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "sort-by": "^1.2.0",
    "styled-components": "^5.3.10",
    "stylis": "^4.1.4",
    "stylis-plugin-rtl": "^2.1.1",
    "tus-js-client": "^2.3.2",
    "use-debounce": "^9.0.4",
    "use-tus": "^0.7.1",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/preset-typescript": "^7.23.0",
    "@types/i18next-browser-languagedetector": "^3.0.0",
    "@types/jest": "^29.5.5",
    "@types/mocha": "^10.0.2",
    "@types/node": "^18.16.3",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-toastify": "^4.1.0",
    "@types/styled-components": "^5.1.26",
    "@types/stylis": "^4.0.2",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.7.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "tailwindcss-dir": "^4.0.0",
    "tailwindcss-rtl": "^0.9.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-pwa": "^0.14.7"
  }
}
